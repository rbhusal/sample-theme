package Calculator;

public class Starter {
public static void main(String[] args){
	Calculator calc = new Calculator();
	
	calc.setNum1(10);
	calc.setNum2(2);
	System.out.println (calc.getNum1() + calc.getNum2() );
	calc.add();
	calc.subtract();
	calc.multiply();
	calc.divide();
	
}
}
